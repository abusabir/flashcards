<div class="jumbotron">
  <h1>All cards</h1>
</div>
<table class="table table-striped">
  <tr>
    <th>#</th>
    <th>Original text</th>
    <th>Translated text</th>
    <th>Review date</th>
  </tr>
  <% @cards.each do |card| %>
    <tr>
        <td><%= card.id %></td>
        <td><%= card.original_text %></td>
        <td><%= card.translated_text %></td>
        <td><%= card.review_date %></td>
    </tr>
  <% end %>
</table>

